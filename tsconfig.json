{
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "lib": [
      "dom",
      "esnext"
    ],
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    ".expo",
    "web-build",
    "dist"
  ],
  "extends": "expo/tsconfig.base"
}

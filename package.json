{
  "name": "trobar-rn",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "node scripts/devStart.js",
    "start:raw": "concurrently -k -i --default-input-target EXPO -n PROXY,EXPO -c yellow,cyan \"npm run ics-proxy\" \"expo start\"",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "npm run web:with-proxy",
    "web:plain": "expo start --web",
    "ics-proxy": "node scripts/icsProxyServer.js",
    "web:with-proxy": "concurrently -k -i --default-input-target WEB -n PROXY,WEB -c yellow,cyan \"npm run ics-proxy\" \"wait-on http://localhost:8787/health && expo start --web\""
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-firebase/app": "^19.0.0",
    "@react-native-firebase/auth": "^19.0.0",
    "@react-native-firebase/firestore": "^19.0.0",
    "@react-native-picker/picker": "2.6.1",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.4.1",
    "cors": "^2.8.6",
    "expo": "~50.0.14",
    "expo-image-picker": "~14.7.1",
    "expo-location": "~16.5.5",
    "expo-status-bar": "~1.11.1",
    "express": "^5.2.1",
    "firebase": "^12.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-maps": "1.10.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@types/react": "~18.2.45",
    "babel-preset-expo": "^10.0.0",
    "concurrently": "^9.2.1",
    "typescript": "^5.1.3",
    "wait-on": "^9.0.3"
  },
  "private": true
}
